#include<iostream>
#include<string.h>
using namespace std;
   int mumbai;
   int pune;
   int nagpur;
   void budget(int b){
    if(b<=25,00,000){
        cout<<"not accepted"<<endl;
    }else{
        cout<<"congrats "<<endl;

    }
   }
   city(){
    cout<<"which city would you like to see the property"<<endl;
   }
   
int main(){
     char  city;
      double budget;
      int choice;
    cout<<"enter choice"<<endl;
    cin>>city;
    city();
    cout<<"enter your choice"<<endl;
   cin>>choice;
   switch(choice){
    case 1:
    cout<<"pune"<<endl;
    case 2:
    cout<<"mumbai"<<endl;
    case 3:
    cout<<"nagpur"<<endl;
   }
    cout<<"enter your budget"<<endl;
    cin>>budget;
    budget(budget);
    
    return 0;

}